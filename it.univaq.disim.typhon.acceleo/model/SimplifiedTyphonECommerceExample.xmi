        type="//@dataTypes.5"/>
    <attributes
        name="id"
        type="//@dataTypes.5"/>
    <relations
        name="orderProducts"
        type="//@dataTypes.2"
        opposite="//@dataTypes.2/@relations.0"/>
    <identifer
        attributes="//@dataTypes.0/@attributes.2"/>
