  <dataTypes
      xsi:type="typhonml:Entity"
      name="Product"
      genericList="//@databases.2/@tables.0">
    <attributes
        name="description"
        type="//@dataTypes.3"/>
    <attributes
        name="id"
        type="//@dataTypes.5"/>
    <relations
        name="productOrders"
        type="//@dataTypes.1"
        opposite="//@dataTypes.1/@relations.0"/>
    <identifer
        attributes="//@dataTypes.0/@attributes.2"/>
