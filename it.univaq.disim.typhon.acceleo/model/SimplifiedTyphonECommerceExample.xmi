  </databases>
  <dataTypes
      xsi:type="typhonml:Entity"
      name="User"
      genericList="//@databases.0/@tables.0">
    <attributes
        name="name"
        type="//@dataTypes.3"/>
    <attributes
        name="surname"
        type="//@dataTypes.3"/>
    <attributes
        name="id"
        type="//@dataTypes.5"/>
    <relations
        name="orders"
        type="//@dataTypes.1"/>
    <identifer
        attributes="//@dataTypes.0/@attributes.2"/>
  </dataTypes>
  <dataTypes
