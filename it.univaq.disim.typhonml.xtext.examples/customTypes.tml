
CustomDataType jpeg {
	elements{
		DataTypeItem date{
			type date
			implementation DataTypeImplementationPackage{
				location 'it.univaq.disim.Date'
			}
		},
		DataTypeItem thumbnail{
			type blob
			implementation DataTypeImplementationPackage{
				location 'it.univaq.disim.Blob'
			}
		}
	}
}




