<?xml version="1.0" encoding="UTF-8"?>
<typhonml:Model
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:typhonml="http://org.typhon.dsls.typhonml.sirius"
    xsi:schemaLocation="http://org.typhon.dsls.typhonml.sirius /it.univaq.disim.typhonml/model/typhonml.ecore">
  <databases
      xsi:type="typhonml:RelationalDB"
      name="MySQL">
    <tables
        name="clientsTable"
        entity="//@dataTypes.0"
        db="//@databases.0"/>
  </databases>
  <databases
      xsi:type="typhonml:DocumentDB"
      name="MongoDB">
    <collections
        name="ordersCollection"
        entity="//@dataTypes.1"/>
  </databases>
  <dataTypes
      xsi:type="typhonml:Entity"
      name="User">
    <attributes
        name="name"
        type="//@dataTypes.0"/>
    <attributes
        name="surname"
        type="//@dataTypes.0"/>
  </dataTypes>
  <dataTypes
      xsi:type="typhonml:Entity"
      name="Order">
    <attributes
        name="date"
        type="//@dataTypes.1"/>
    <attributes
        name="totalAmount"
        type="//@dataTypes.1"/>
  </dataTypes>
  <dataTypes
      xsi:type="typhonml:Entity"
      name="Review">
    <attributes
        name="rating"
        type="//@dataTypes.2"/>
    <attributes
        name="title"
        type="//@dataTypes.2"/>
  </dataTypes>
  <dataTypes
      xsi:type="typhonml:Entity"
      name="Comment">
    <attributes
        name="content"
        type="//@dataTypes.3"/>
  </dataTypes>
  <dataTypes
      xsi:type="typhonml:Entity"
      name="CreditCard">
    <attributes
        name="number"
        type="//@dataTypes.4"/>
    <attributes
        name="expiryDate"
        type="//@dataTypes.4"/>
  </dataTypes>
</typhonml:Model>
