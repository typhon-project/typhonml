<?xml version="1.0" encoding="UTF-8"?>
<typhonml:Model
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:typhonml="http://org.typhon.dsls.typhonml.sirius">
  <databases
      xsi:type="typhonml:RelationalDB"
      name="MySQL">
    <tables
        name="clientsTable"
        db="//@databases.0"
        entity="//@dataTypes.0"/>
  </databases>
  <databases
      xsi:type="typhonml:DocumentDB"
      name="MongoDB">
    <collections
        name="ordersCollection"
        entity="//@dataTypes.1"/>
  </databases>
  <dataTypes
      xsi:type="typhonml:Entity"
      name="User">
    <attributes
        name="name"
        type="//@dataTypes.5"/>
    <attributes
        name="surname"
        type="//@dataTypes.5"/>
    <attributes
        name="id"
        type="//@dataTypes.6"/>
    <relations
        name="Test"
        type="//@dataTypes.1"/>
    <functionalTags
        name="sort"/>
  </dataTypes>
  <dataTypes
      xsi:type="typhonml:Entity"
      name="Order">
    <attributes
        name="date"
        type="//@dataTypes.7"/>
    <attributes
        name="totalAmount"
        type="//@dataTypes.6"/>
  </dataTypes>
  <dataTypes
      xsi:type="typhonml:Entity"
      name="Review">
    <attributes
        name="rating"
        type="//@dataTypes.6"/>
    <attributes
        name="title"
        type="//@dataTypes.5"/>
  </dataTypes>
  <dataTypes
      xsi:type="typhonml:Entity"
      name="Comment">
    <attributes
        name="content"
        type="//@dataTypes.5"/>
  </dataTypes>
  <dataTypes
      xsi:type="typhonml:Entity"
      name="CreditCard">
    <attributes
        name="number"
        type="//@dataTypes.6"/>
    <attributes
        name="expiryDate"
        type="//@dataTypes.7"/>
  </dataTypes>
  <dataTypes
      xsi:type="typhonml:PrimitiveDataType"
      name="String"/>
  <dataTypes
      xsi:type="typhonml:PrimitiveDataType"
      name="Integer"/>
  <dataTypes
      xsi:type="typhonml:PrimitiveDataType"
      name="Date"/>
</typhonml:Model>
